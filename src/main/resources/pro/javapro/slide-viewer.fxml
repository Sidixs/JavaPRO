<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPaneSlider" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pro.javapro.SlideViewerController">
   <children>
      <Button fx:id="nextBtn" layoutX="470.0" layoutY="540.0" mnemonicParsing="false" onAction="#NextBtnClicked" text="Następna" />
      <Button fx:id="previousBtn" layoutX="365.0" layoutY="540.0" mnemonicParsing="false" onAction="#PreviousBtnClicked" text="Poprzednia" />
      <ImageView fx:id="myimageview" fitHeight="450.0" fitWidth="777.0" layoutX="61.0" layoutY="66.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="backBtn" layoutX="26.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackBtnClicked" text="Wróć" />
      <Label fx:id="labelField" alignment="CENTER" layoutX="213.0" layoutY="17.0" prefHeight="18.0" prefWidth="473.0" />
      <Button fx:id="firstBtn" layoutX="277.0" layoutY="540.0" mnemonicParsing="false" onAction="#FirstBtnClicked" text="Pierwsza" />
      <Button fx:id="lastBtn" layoutX="565.0" layoutY="540.0" mnemonicParsing="false" onAction="#LastBtnClicked" text="Ostatnia" />
   </children>
</AnchorPane>
